{
  "name": "@covenant-rpc/server",
  "module": "./index.ts",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "covenant-sidekick": "./bin/sidekick.ts"
  },
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts"
    },
    "./*": {
      "types": "./*.ts",
      "import": "./*.ts"
    }
  },
  "scripts": {
    "build": "bun ../../scripts/build-package.ts .",
    "sidekick": "bun ./bin/sidekick.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@covenant-rpc/core": "^1.0.0",
    "@covenant-rpc/ion": "^2.0.0",
    "@covenant-rpc/request-serializer": "^2.0.0",
    "commander": "^12.1.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1"
  }
}
